{
  "analysis": {
    "tools": [
      {
        "id": "checkstyle",
        "pattern": "target/checkstyle.xml"
      },
      {
        "id": "spotbugs",
        "pattern": "target/spotbugsXml.xml"
      }
    ],
    "typesIgnorePattern": "HideUtilityClassConstructorCheck|UseUtilityClass|UseVarargs",
    "fileFilter": "Assignment[0-9][0-9].java",
    "maxScore": 100,
    "errorImpact": -5,
    "highImpact": -3,
    "normalImpact": -2,
    "lowImpact": -1
  },
  "tests": {
    "maxScore": 100,
    "passedImpact": 0,
    "failureImpact": -5,
    "skippedImpact": -1
  },
  "coverage": {
    "maxScore": 100,
    "coveredPercentageImpact": 0,
    "missedPercentageImpact": -1
  },
  "pit": {
    "maxScore": 100,
    "detectedImpact": 0,
    "undetectedImpact": 0,
    "detectedPercentageImpact": 0,
    "undetectedPercentageImpact": -1
  }
}
